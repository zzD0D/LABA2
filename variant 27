№1)
#include <iostream>
#include <cmath>
using namespace std;
int f(int v1, int v2, int t){
int s = v1*t+v2*t;
return s;
}
int main(){
int v1=40;
int v2=60;
int t=2;
cout << f(v1,v2,t);
return 0;
}

