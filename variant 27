№3)
#include <iostream>
using namespace std;

int main() {
int a;
cout << "vvedite chislo:" << a;
cin >> a;
if (a % 2 == 0)
{
    cout << "eto chetnoe chislo \n";
}
else
{
    cout << "eto nechetnoe chislo \n";
}
}
№4)
#include <iostream>
#include <cmath>
using namespace std;
double f(double x, double y){
double num = (pow(2*x,3)+1)/sqrt(x)+pow(x,sqrt(y));
return num;
}
int main()
{
    double x;
   cout<<"Vvedite x:";
   cin>>x;
      double y;
   cout<<"Vvedite y:";
   cin>>y;
   double a = f(x,y);
   cout<<"Otvet: "<<a;
   return 0;
}
№5)
#include <iostream>
#include <cmath>
#define PI 3.14
#define E 2.71
using namespace std;
double f(double x)
{
double num = pow(x,2);
double denum = pow(cos(x),2)+pow(sin(x),2);
return num/denum;
}
int main()
{
    double x;
   cout<<"Vvedite x:";
   cin>>x;
   double a = f(x);
   cout<<"Otvet: "<<a;
   return 0;
}
